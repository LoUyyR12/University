 solid midnightblue ;\r\n            border-radius: 10px;\r\n            padding: 17px;\r\n            margin-top: 6px;\r\n        }\r\n    }\r\n    .cart{\r\n        display: flex;\r\n        align-items: center;\r\n        margin-left: 70px;\r\n        &-counter {\r\n            font-weight: 600;\r\n            background-color: $accent-color;\r\n            color: #fff;\r\n            font-size: 22px;\r\n            margin-right: 10px;\r\n            padding: 10px 15px;\r\n            border-radius: 0.8rem;\r\n        }\r\n        cursor: pointer;\r\n    }\r\n}\r\n\r\n// Specific Styles\r\n\r\n.background {\r\n    // background: url('/src/img/background-handmade.jpg');\r\n    background-position: center;\r\n    .navigation {\r\n        .brand-name {\r\n            text-align: initial;\r\n            color: $dark-accent-color;\r\n        }\r\n        .menu {\r\n            display: flex;\r\n            justify-content: space-evenly;\r\n            align-items: center;\r\n            margin: 0 auto 1%;\r\n            list-style: none;\r\n            \r\n\r\n            &-item {\r\n                width: 200px;\r\n                background: $light-accent-color;\r\n                padding: 3px;\r\n                border-radius: 14%;\r\n                font-size: 30px;\r\n                font-weight: 600;\r\n                text-align: center;\r\n            }\r\n            &-link {\r\n                text-decoration: none;\r\n                color: $dark-accent-color;\r\n                :hover {\r\n                    text-decoration: underline;\r\n                } \r\n            }\r\n        }\r\n    }\r\n    .description {\r\n        &-title {\r\n            font-size: 2.2rem;\r\n            text-align: end;\r\n        }\r\n        &-content {\r\n            font-size: 30px;\r\n        }\r\n    }\r\n    .hand-made_about {\r\n        display: flex;\r\n        align-items: center;\r\n    \r\n        .text-about  {\r\n            margin-left: 50px;\r\n        }\r\n    }\r\n}\r\n\r\n.customers {\r\n    &-title{\r\n        font-size: 50px;\r\n        text-align: center;\r\n    }\r\n    .shop {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        align-items: center;\r\n        justify-content: space-evenly;\r\n        margin-right: 5px;\r\n        margin: 0 auto 1%;\r\n        list-style: none;\r\n        border: 1px dashed $accent-color;\r\n        border-radius: 6px;\r\n        padding: 15px;\r\n        overflow: hidden;\r\n        text-align: center;\r\n    }\r\n\r\n}\r\n\r\n.about {\r\n    margin: 20px 5px;\r\n    &_1 {\r\n        display: flex;\r\n        align-items: center;\r\n        margin: 20px 5px;\r\n        &-content {\r\n            font-size: 22px;\r\n            margin-right: 20px;\r\n        }\r\n    }\r\n    &_2 {\r\n        display: flex;\r\n        &-content{\r\n            margin-left: 20px;\r\n            font-size: 22px;\r\n        }\r\n    } \r\n}\r\n\r\n.contacts {\r\n    .join {\r\n        text-align: center;\r\n        background-color: $header-color;\r\n        color: $dark-accent-color;\r\n        font-size: 30px;\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"a4368c56ffc91b85e689899aa5363233","cacheData":{"env":{}}}